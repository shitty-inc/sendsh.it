{
  "name": "sendsh.it",
  "version": "1.0.0",
  "description": "Send and recieve files encrypted in the browser",
  "main": "server/dist/server.lambda.js",
  "scripts": {
    "start": "npm run build && node ./server/dist/server.local.js",
    "build": "./node_modules/.bin/babel ./server/src --experimental --source-maps-inline -d ./server/dist",
    "watch": "nodemon ./server/dist/server.local.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/threesquared/sendsh.it"
  },
  "homepage": "https://sendsh.it/",
  "author": "Ben Speakman",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.139.0",
    "aws-serverless-express": "^3.0.2",
    "express": "^4.12.4",
    "gulp": "^3.8.11",
    "gulp-bower": "0.0.10",
    "gulp-concat": "^2.5.2",
    "gulp-minify-css": "^1.1.1",
    "gulp-order": "^1.1.1",
    "gulp-uglify": "^1.2.0",
    "main-bower-files": "^2.9.0",
    "multiparty": "^4.1.2",
    "shortid": "^2.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.13.3"
  }
}
